{
  "name": "@syngenta-digital/package-node-cropwise-sustainability-data-models",
  "version": "1.1.21",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf ./lib",
    "build:model": "tsc -p tsconfig-data-model.json",
    "build:serverless": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "typeorm": "typeorm-ts-node-esm -d ./src/config/dbInstanceLocal.ts",
    "serverless": "serverless",
    "deploy": "sls deploy",
    "deploy:fn": "sls deploy function -f",
    "invoke": "sls invoke local -f",
    "invoke:watch": "sls invoke local --watch -f",
    "offline": "sls offline start",
    "clean:serverless": "rimraf ./dist",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/db-migration",
    "migration:run:v1": "npm run clean:serverless && npm run build:serverless && sls invoke local -f runMigration_v1_v2 --data v1",
    "migration:run": "npm run clean:serverless && npm run build:serverless && sls invoke local -f runMigration"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@types/source-map-support": "^0.5.6",
    "@types/webpack-node-externals": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "serverless": "^3.33.0",
    "serverless-offline": "^12.0.4",
    "serverless-webpack": "^5.11.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.369.0",
    "graphql": "^16.3.0",
    "pg": "^8.11.1",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-plugin-bind-deployment-id": "^2.0.3",
    "serverless-plugin-warmup": "^8.2.1",
    "typeorm": "^0.3.17",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.2"
  }
}
